pipeline {
    agent any
    tools {
        nodejs 'nodejs'
    }
    stages {
        stage('installing') {
            steps {
                sh 'npm install'
            }
        }
        stage('starting') {
            steps {
                sh 'npm start'
            }
        }
        stage('packing') {
            steps {
                sh 'npm pack'
            }
        }
    }
}
